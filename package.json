{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "## Setup npm install npm run build npm start",
  "main": "Basic reusable React Boilerplate",
  "scripts": {
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
    "build": "npm run build-server && npm run build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js",
    "test": "jest ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deefourple/react-boilerplate.git"
  },
  "author": "Dave Dehghani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deefourple/react-boilerplate/issues"
  },
  "homepage": "https://github.com/deefourple/react-boilerplate#readme",
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "webpack": "^3.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "jest": "^23.5.0"
  }
}
